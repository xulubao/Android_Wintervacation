<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="5.0dip" />
            <gradient android:startColor="@color/voicebg2" android:endColor="@color/voicebg2" android:angle="270.0" android:centerY="0.75" android:centerColor="@color/voicebg2" />
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="5.0dip" />
                <solid android:color="#ff50a6f2" />
            </shape>
        </clip>
    </item>
</layer-list>